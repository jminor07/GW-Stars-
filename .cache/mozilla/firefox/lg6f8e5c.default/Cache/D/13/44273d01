function mkemail(parts) {
    var sep = ".";
    var str = arguments[0];
    for ( var i = 1 ; i < arguments.length ; i++ ) {
        if ( arguments[i] == "" ) {
            sep = String.fromCharCode(29+35) ;
           continue ;
        } else {
            str = str.concat ( sep ) ;
            str = str.concat ( arguments[i] ) ;
            sep = ".";
        }
    }
    return(str);
}

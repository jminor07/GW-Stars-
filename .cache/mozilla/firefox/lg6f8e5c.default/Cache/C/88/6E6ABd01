var newAction=action;if(redirect!=''){newAction+=encodeURIComponent(redirect);}
if(aForm.getAttributeNode){aForm.getAttributeNode("action").value=newAction;}else{aForm.action=newAction;}
aForm.method="post";aForm.submit();}

function render(aTerms){for(var i=0;i<aTerms.length;i++){var t=aTerms[i];var regex=new RegExp("\"([-a-zA-Z0-9' ]+)\"(~([0-9]*))?|([\u0590-\u05FF]+)");var match=regex.exec(t);if(match==null){renderTerm(t);var hyphens=/-+/;if(hyphens.test(t)){var phrase=t.replace(/-+/g," ");renderPhrase(phrase,1);}
continue;}

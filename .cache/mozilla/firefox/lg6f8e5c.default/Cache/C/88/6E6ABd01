if(match[3]){proximity=parseInt(match[3]);}
renderPhrase(terms,proximity);}}
function renderTerm(aTerm){var wids=text2id(aTerm);for(var w=0;w<wids.length;w++){var occurs=findOccurIndexes(wids[w]);var div=document.getElementById("pageImageContainer");for(var i=0;i<occurs.length;i++){var idx=occurs[i];var span=newSpan(idx);maybeAppend(div,span);}}}
function renderPhrase(aPhrase,aProximity){var wids=[];var ts=tokenize(aPhrase," ");for(var i=0;i<ts.length;i++){var wid=text2id(ts[i]);if(wid.length==0){return;}
wids.push(text2id(ts[i]));}

function findAllOccurIndexes(wids){var allOccurs=[];for(var i=0;i<wids.length;i++){var oneResult=findOccurIndexes(wids[i]);for(var r=0;r<oneResult.length;r++){allOccurs.push(oneResult[r]);}}

function findOccurIndexes(aWid){var result=[];if(aWid==-1){return result;}
for(var i=0;i<wis.length;i++){if(wis[i]==aWid){result.push(i);}}
return result;}
function getLine(aOccur){for(var i=0;i<lss.length;i++){if(lss[i]>aOccur){return i-1;}}

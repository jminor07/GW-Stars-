! version for CaHK vs <Fe>
erase

limits 1.3 2.9 11.5 25
expand 1.3
box
xlabel <Fe> (\AA)
ylabel CaHK (\AA)
relocate 1.4 24
label (a.)
relocate 1.5 12.5
ptype 4 0
expand 3.0
dot
relocate 1.5 13
ptype 4 0 
expand 1.0001
dot
relocate 1.55 12.5
putlabel 6 300 km s^{-1}
relocate 1.55 13
putlabel 6 100 km s^{-1}
angle 35
relocate 2.5 23
putlabel 6 [M/H] = 0.25
relocate 1.65 19.9
angle 55
putlabel 4 [M/H] = -0.25
angle 0

! Sanchez-Blazquez
data temp.plot
read { vd 1 mgb 14 fe52 15 fe53 16 cahk 40 fe52e 94 fe53e 95 cahke 119 } 
set fe = 0.5* (fe52 + fe53)
set fee = 0.35*(fe53e + fe52e)
set efactor = vd/100
expand efactor

ctype cyan
ptype 20 0
points fe cahk
expand 0
errorbar fe cahk cahke 2
errorbar fe cahk cahke 4
errorbar fe cahk fee 1
errorbar fe cahk fee 3


! Jed's galaxies
data indexm.txt
read { vd 1 mgb 14 fe52 15 fe53 16 cahk 40 fe52e 94 fe53e 95 cahke 119 } 
set fe = 0.5* (fe52 + fe53)
set fee = 0.35*(fe53e + fe52e)
set efactor = vd/100
expand efactor

ctype red
ptype 4 0
points fe cahk
expand 0
errorbar fe cahk cahke 2
errorbar fe cahk cahke 4
errorbar fe cahk fee 1
errorbar fe cahk fee 3

! Trager
data Coma.plot
read { vd 1 mgb 14 fe52 15 fe53 16 cahk 40 fe52e 94 fe53e 95 cahke 119 } 
set fe = 0.5* (fe52 + fe53)
set fee = 0.35*(fe53e + fe52e)
set efactor = vd/100
expand efactor

ctype blue
ptype 20 3
points fe cahk
expand 0
errorbar fe cahk cahke 2
errorbar fe cahk cahke 4
errorbar fe cahk fee 1
errorbar fe cahk fee 3

! Sloan
data tmeasure.out
read { vd 1 mgb 14 fe52 15 fe53 16 cahk 40 fe52e 94 fe53e 95 cahke 119 } 
set fe = 0.5* (fe52 + fe53)
set fee = 0.35*(fe53e + fe52e)
set efactor = vd/100
expand efactor

ctype green
ptype 6 2
points fe cahk
expand 0
errorbar fe cahk cahke 2
errorbar fe cahk cahke 4
errorbar fe cahk fee 1
errorbar fe cahk fee 3


ctype default

